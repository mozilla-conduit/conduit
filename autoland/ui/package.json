{
  "devDependencies": {
    "enzyme": "^2.7.1",
    "neutrino": "^4.3.0",
    "neutrino-preset-airbnb-base": "4.3.1",
    "neutrino-preset-jest": "^4.3.0",
    "neutrino-preset-react": "^4.2.3",
    "react-addons-test-utils": "^15.4.2"
  },
  "main": "src/index.js",
  "config": {
    "presets": [
      "neutrino-preset-react",
      "conduit-preset.js"
    ],
    "html": {
      "scripts": ["/scripts/environment.js"],
      "links": [
        {
          "href": "/images/blank.gif",
          "rel": "icon",
          "type": "image/x-icon"
        }
      ]
    }
  },
  "scripts": {
    "build": "neutrino build",
    "lint": "./node_modules/eslint/bin/eslint.js --ext .js,.jsx src/ test/",
    "start": "HOST=0.0.0.0 PORT=4000 neutrino start",
    "test": "neutrino test --presets neutrino-preset-node neutrino-preset-jest"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.23.0",
    "copy-webpack-plugin": "3.0.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  }
}
